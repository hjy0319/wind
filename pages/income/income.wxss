.container {
  top: 130rpx;
  display: flex;
  flex-direction: column;
}

.form {
  padding: 0 30rpx;
  border-top: 2px solid #e4e4e4;
  font-size: 14px;
  width: 90%;
}

.ipt-wrap{
  min-height: 100rpx;
  line-height: 100rpx;
  display: flex;
  align-items: center;
}
.ipt-wrap label{
  min-width: 120rpx;
}

.row {
  min-height: 100rpx;
  line-height: 100rpx;
  border-bottom:2px solid #e4e4e4;
  align-items: center;
  display: flex;
  justify-content: space-between;
}

.text {
  display:flex; 
  width:206rpx;
  justify-content:center;
  font-weight: 700;
  font-size: 19px;
  color: #635f5f;
}
.combtn{
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  margin: 0 auto;
  border-radius: 5rpx;
  position: fixed;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  font-size: 16px;
  color: #fff;
  width: 100%;
  background: #229dfe;
}

.canvas {
  width: 100%;
  height: 300px;
  margin-top: 50px;
  /* border-top: 2px solid #e4e4e4; */
}

.divLine {
  position: fixed;
  top: 125rpx;
  width: 100%;
  height: 5rpx;
  background: #C0C0C0;
}

.header {
  position: fixed;
  top: 0rpx;
  width: 750rpx;
  height: 125rpx;
  flex-wrap: nowrap;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #F8F8FF;
}
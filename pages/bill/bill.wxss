.header {
  position: fixed;
  top: 0rpx;
  width: 750rpx;
  height: 125rpx;
  flex-wrap: nowrap;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #F8F8FF;
}

.list {
  width: 100%;
  bottom: 0;
  top: 135rpx;
  /* background-color: red; */
  position: absolute;
}

.row {
  min-height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #e4e4e4;
  align-items: center;
  display: flex;
  justify-content: space-between;
  position: relative;
}

.divLine {
  position: fixed;
  top: 125rpx;
  width: 100%;
  height: 5rpx;
  background: #C0C0C0;
}
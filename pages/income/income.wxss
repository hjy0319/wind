.container {
  height: 100%;
  min-height: 100vh;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  background: #f5f5f5;
  overflow-x: hidden;
}

.form {
  padding: 0 30rpx;
  border-top: 2px solid #e4e4e4;
  font-size: 14px;
  width: 90%;
}

.ipt-wrap{
  min-height: 100rpx;
  line-height: 100rpx;
  display: flex;
  align-items: center;
}
.ipt-wrap label{
  min-width: 120rpx;
}

.row {
  min-height: 100rpx;
  line-height: 100rpx;
  border-bottom:2px solid #e4e4e4;
  align-items: center;
  display: flex;
  justify-content: space-between;
}

.text {
  display:flex; 
  width:206rpx;
  justify-content:center;
  color: #635f5f;
}
.combtn{
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  margin: 0 auto;
  border-radius: 5rpx;
  position: fixed;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  font-size: 16px;
  color: #fff;
  width: 100%;
  background: #229dfe;
}

.canvas {
  width: 100%;
  height: 300px;
  margin-top: 30px;
  border-top: 2px solid #e4e4e4;
}